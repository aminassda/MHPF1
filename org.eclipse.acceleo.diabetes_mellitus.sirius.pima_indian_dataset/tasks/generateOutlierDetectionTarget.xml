<?xml version="1.0" encoding="UTF-8"?>

<!-- Place this file in the target project and call it with "External Tools > Run As > Ant Build" -->
<!-- You have to change the MODEL and the TARGET values -->

<project basedir="." default="generateOutlierDetectionSample" name="org.eclipse.acceleo.diabetes_mellitus.sirius.pima_indian_datasetSample">
	<import file="../../org.eclipse.acceleo.diabetes_mellitus.sirius.pima_indian_dataset/tasks/generateOutlierDetection.xml"/>

    <!-- Change the values of MODEL and TARGET to point to your model and output folder -->
	<property name="MODEL" value="${basedir}/model/file.xmi"/>
	<property name="TARGET" value="${basedir}/src-gen"/>

	<target name="generateOutlierDetectionSample" description="Generate files in 'TARGET'">
		<antcall target="generateOutlierDetection" >
			<param name="model" value="${MODEL}"/>
			<param name="target" value="${TARGET}"/>
		</antcall>
	</target>
</project>
